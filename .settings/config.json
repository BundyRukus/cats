{
    "src": "src/**/*.ts",
    "main": "index.html",
    "buildOnSave": false,
    "compiler": {
        "noLib": false,
        "removeComments": false,
        "noImplicitAny": false,
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "propagateEnumConstants": false,
        "allowAutomaticSemicolonInsertion": true,
        "target": 1,
        "module": 0,
        "outDir": null,
        "out": null
    },
    "codingStandards": {
        "newLineMode": "auto",
        "useSoftTabs": true,
        "tabSize": 4,
        "useLint": false,
        "lintFile": null
    },
    "customBuild": {
        "name": "Jake Build Process",
        "command": "jake",
        "directory": null,
        "environment": null,
        "ownConsole": false
    },
    "customRun": {
        "name": null,
        "command": null,
        "directory": null,
        "environment": null,
        "ownConsole": false
    },
    "documentation": {
        "includeDeclarations": false,
        "outputDirectory": "api_doc",
        "readme": "README.md",
        "theme": "default"
    }
}