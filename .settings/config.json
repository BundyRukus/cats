{
    "version": "1.0",
    "main": "static/index.html",
    "src": "src/**/*.ts",
    "customBuild": {
        "name" : "Jake Build Process",
        "command": "jake"
    },
    "compiler": {
        "useDefaultLib": true,
        "emitComments": true,
        "noImplicitAny" : false,
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "codeGenTarget": 1,
        "moduleGenTarget": 0
    },
   "codingStandards": {
        "newLineMode": "unix",
        "useSoftTabs": true,
        "tabSize": 4,
        "useLint": false,
        "lintFile": null
    }
         
}
