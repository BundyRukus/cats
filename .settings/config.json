{
    "version": "0.8",
    "main": "static/index.html",
    "src": "src/**/*.ts",
    "customBuild": {
        "command": "jake"
    },
    "compiler": {
        "useDefaultLib": true,
        "emitComments": false,
        "noImplicitAny" : false,
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "codeGenTarget": 1,
        "moduleGenTarget": 0
    },
    "completionMode": "loose",
    "minify": false,
    "rememberOpenFiles": true
}