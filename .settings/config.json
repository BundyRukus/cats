{
    "version": "1.0",
    "main": "static/index.html",
    "src": "src/**/*.ts",
    "customBuild": {
        "command": "jake"
    },
    "compiler": {
        "useDefaultLib": true,
        "emitComments": true,
        "noImplicitAny" : false,
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "codeGenTarget": 1,
        "moduleGenTarget": 0
    }
}
