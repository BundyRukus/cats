{
    "src": "src/**/*.ts",
    "main": "static/index.html",
    "buildOnSave": false,
    "compiler": {
        "noLib": false,
        "removeComments": false,
        "noImplicitAny": false,
        "generateDeclarationFiles": false,
        "mapSourceFiles": false,
        "propagateEnumConstants": false,
        "allowAutomaticSemicolonInsertion": true,
        "codeGenTarget": 1,
        "moduleGenTarget": 0,
        "outDirOption": null,
        "outFileOption": null
    },
    "codingStandards": {
        "newLineMode": "auto",
        "useSoftTabs": true,
        "tabSize": 4,
        "useLint": false,
        "lintFile": null
    },
    "customBuild": {
        "name": "Jake Build Process",
        "command": "jake",
        "directory": null,
        "environment": null,
        "ownConsole": false
    },
    "customRun": {
        "name": null,
        "command": null,
        "directory": null,
        "environment": null,
        "ownConsole": false
    }
}